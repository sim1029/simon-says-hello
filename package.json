{
	"name": "simon-says-hello",
	"description": "Portfolio website and blog of Simon Schueller",
	"version": "1.0.0",
	"author": "Simon Schueller",
	"dependencies": {
		"@mdx-js/mdx": "^1.6.22",
		"@mdx-js/react": "^1.6.22",
		"@react-icons/all-files": "^4.1.0",
		"@sendgrid/mail": "^7.7.0",
		"classnames": "^2.3.1",
		"gatsby": "^4.25.7",
		"gatsby-plugin-image": "^2.0.0",
		"gatsby-plugin-manifest": "^4.25.0",
		"gatsby-plugin-mdx": "^3.19.0",
		"gatsby-plugin-netlify": "^4.3.1",
		"gatsby-plugin-netlify-cms": "^6.0.0",
		"gatsby-plugin-react-helmet": "^5.0.0",
		"gatsby-plugin-react-svg": "^3.1.0",
		"gatsby-plugin-sharp": "^4.0.0",
		"gatsby-remark-copy-linked-files": "^5.25.0",
		"gatsby-remark-images": "^6.25.0",
		"gatsby-remark-relative-images": "^2.0.5",
		"gatsby-source-filesystem": "^4.0.0",
		"gatsby-transformer-remark": "^5.25.1",
		"gatsby-transformer-sharp": "^4.0.0",
		"lodash": "^4.17.15",
		"lodash-webpack-plugin": "^0.11.4",
		"netlify-cli": "^16.9.1",
		"netlify-cms-app": "^2.15.65",
		"prop-types": "^15.6.0",
		"react": "^17.0.0",
		"react-dom": "^17.0.0",
		"react-helmet": "^6.0.0",
		"uuid": "^8.0.0"
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"main": "n/a",
	"scripts": {
		"clean": "gatsby clean",
		"develop": "gatsby develop",
		"start": "run-p start:**",
		"start:app": "npm run develop",
		"start:lambda": "netlify-lambda serve src/functions",
		"build": "gatsby build && netlify-lambda build src/functions",
		"build:app": "gatsby build",
		"build:lambda": "netlify-lambda build src/functions",
		"serve": "gatsby serve",
		"format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint .",
		"lint:fix": "npx eslint --fix --ext .jsx --ext .js src/",
		"prepare": "husky install",
		"commit": "git add . && git commit -m",
		"watch": "nodemon --exec 'kill-port --port 8000 && gatsby develop' --watch gatsby-node.js --watch package.json --watch gatsby-config.js"
	},
	"engines": {
		"node": ">= 14.15.0"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"eslint",
			"prettier --write"
		]
	},
	"devDependencies": {
		"@tailwindcss/typography": "^0.5.2",
		"autoprefixer": "^10.4.7",
		"dotenv": "^16.3.1",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"gatsby-plugin-postcss": "^5.14.0",
		"http-proxy-middleware": "^2.0.6",
		"husky": "^7.0.0",
		"kill-port": "^2.0.1",
		"lint-staged": "^13.0.3",
		"netlify-lambda": "^2.0.16",
		"nodemon": "^2.0.19",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.13",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.11",
		"tailwindcss": "^3.0.24"
	}
}
